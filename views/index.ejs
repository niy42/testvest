    <!-- profit calculator -->
    <script>
        function calculateProfit() {
            const planElement = document.getElementById('plan');
            const investAmountElement = document.getElementById('invest_amount');
            const profitAmountElement = document.getElementById('profit_amount');

            const planRate = parseFloat(planElement.value);
            const investAmount = parseFloat(investAmountElement.value);
            let profitAmount;

            if (!isNaN(planRate) && !isNaN(investAmount)) {
                switch (planElement.options[planElement.selectedIndex].text) {
                    case "Silver Plan":
                        if (investAmount >= 100 && investAmount <= 1000) {
                            profitAmount = investAmount * planRate;
                            profitAmountElement.value = profitAmount.toFixed(2);
                        } else {
                            profitAmountElement.value = "Amount must be between 100 and 1000";
                        }
                        break;
                    case "Gold Plan":
                        if (investAmount >= 1000 && investAmount <= 10000) {
                            profitAmount = investAmount * planRate;
                            profitAmountElement.value = profitAmount.toFixed(2);
                        } else {
                            profitAmountElement.value = "Amount must be between 1000 and 10000";
                        }
                        break;
                    case "Platinum Plan":
                        if (investAmount >= 10000 && investAmount <= 50000) {
                            profitAmount = investAmount * planRate;
                            profitAmountElement.value = profitAmount.toFixed(2);
                        } else {
                            profitAmountElement.value = "Amount must be between 10000 and 50000";
                        }
                        break;
                    case "Diamond Plan":
                        if (investAmount >= 5000 && investAmount <= 50000) {
                            profitAmount = investAmount * planRate;
                            profitAmountElement.value = profitAmount.toFixed(2);
                        } else {
                            profitAmountElement.value = "Amount must be between 5000 and 50000";
                        }
                        break;
                    case "State Holders":
                        if (investAmount >= 10000 && investAmount <= 200000) {
                            profitAmount = investAmount * planRate;
                            profitAmountElement.value = profitAmount.toFixed(2);
                        } else {
                            profitAmountElement.value = "Amount must be between 10000 and 200000";
                        }
                        break;
                    case "Share Holder":
                        if (investAmount >= 20000 && investAmount <= 500000) {
                            profitAmount = investAmount * planRate;
                            profitAmountElement.value = profitAmount.toFixed(2);
                        } else {
                            profitAmountElement.value = "Amount must be between 20000 and 500000";
                        }
                        break;
                    default:
                        profitAmountElement.value = "0.00";
                        break;
                }
            } else {
                profitAmountElement.value = "0.00";
            }
        }
    </script>


