    <script>
        $(document).ready(function () {
            // Edit button click handler
            $(document).on('click', '.user-info-inner_header .edit-button', function () {
                var $section = $(this).closest('.user-info-inner');
                var $list = $section.find('.user-info-inner_list');

                // Toggle edit mode
                $list.find('.value').each(function () {
                    var value = $(this).text().trim();
                    $(this).html('<input type="text" value="' + value + '">');
                });

                // Replace Edit button with Save/Cancel buttons
                $(this).replaceWith('<button type="button" class="save-button"><i class="ti-save"></i> Save</button> ' +
                    '<button type="button" class="cancel-button"><i class="ti-close"></i> Cancel</button>');
            });

            // Save button click handler (using event delegation)
            $(document).on('click', '.user-info-inner_header .save-button', function () {
                var $section = $(this).closest('.user-info-inner');
                var $list = $section.find('.user-info-inner_list');

                $list.find('input[type="text"]').each(function () {
                    var value = $(this).val().trim();
                    $(this).parent().html(value);
                });

                // Replace Save/Cancel buttons with Edit button
                $section.find('.user-info-inner_header').append('<button type="button" class="edit-button"><i class="ti-pencil-alt"></i> Edit</button>');

                // Remove Save/Cancel buttons
                $(this).remove();
                $('.cancel-button').remove();
            });

            // Cancel button click handler (using event delegation)
            $(document).on('click', '.user-info-inner_header .cancel-button', function () {
                var $section = $(this).closest('.user-info-inner');
                var $list = $section.find('.user-info-inner_list');

                $list.find('input[type="text"]').each(function () {
                    var value = $(this).parent().text().trim();
                    $(this).parent().html(value);
                });

                // Replace Save/Cancel buttons with Edit button
                $section.find('.user-info-inner_header').append('<button type="button" class="edit-button"><i class="ti-pencil-alt"></i> Edit</button>');

                // Remove Save/Cancel buttons
                $(this).remove();
                $('.save-button').remove();
            });
        });
    </script>
